        </tr>
    </thead>
        
    <tbody>

        <tr>
        {{#each rows}}
        <th scope="row">#</th>
        <td>{{this.rut}}</td>
        <td>{{this.nombre}}</td>
        <td>{{this.email}}</td>
        <td>{{this.curso_fk}}</td>
        <td>{{this.fecha}}</td>
        </tr>
       {{/each}}
          
    </tbody>
    </table>
